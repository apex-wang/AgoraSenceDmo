<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="radio_style">
        <item name="android:paddingLeft">8dp</item>
        <item name="android:button">@drawable/room_create_encrypt_bg</item>
        <item name="buttonStyle">@style/Widget.AppCompat.Button.Borderless</item>
        <item name="background">@color/transparent</item>
    </style>

    <style name="edit_encryption">
        <item name="android:layout_marginStart">16dp</item>
    </style>

    <style name="chatroom_seat_roundStyle">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="RoundedStyle">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">16dp</item>
    </style>

    <style name="measureSoftKeyAppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="em_login_et_input">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">52dp</item>
        <item name="android:layout_marginLeft">30dp</item>
        <item name="android:layout_marginRight">30dp</item>
        <item name="android:maxWidth">400dp</item>
        <item name="android:background">@drawable/demo_login_et_bg</item>
        <item name="android:paddingLeft">18dp</item>
        <item name="android:paddingRight">18dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:maxLines">1</item>
        <item name="android:inputType">text</item>
        <item name="android:textColor">@color/color_3c4267</item>
    </style>

    <declare-styleable name="MyCheckBox">
        <attr name="android:button" />
        <attr name="attr_drawableWidth" format="dimension" />
        <attr name="attr_drawableHeight" format="dimension" />
    </declare-styleable>
</resources>